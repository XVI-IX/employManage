{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/employManage/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/employManage/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/employManage",
  "projects": {
    "employManage": {
      "type": "application",
      "root": "apps/employManage",
      "entryFile": "main",
      "sourceRoot": "apps/employManage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/employManage/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "employees": {
      "type": "application",
      "root": "apps/employees",
      "entryFile": "main",
      "sourceRoot": "apps/employees/src",
      "compilerOptions": {
        "tsConfigPath": "apps/employees/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    }
  }
}